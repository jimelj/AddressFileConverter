{
  "name": "address-file-converter",
  "version": "1.0.0",
  "description": "A web app for converting Excel files to a specific format",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^2.0.0-rc.2",
    "xlsx": "^0.18.5",
    "cors": "^2.8.5",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": ["excel", "converter", "web-app"],
  "author": "",
  "license": "MIT"
} 